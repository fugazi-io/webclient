{
  "name": "@fugazi/webclient",
  "version": "1.0.16",
  "description": "web based terminal application for executing local and remote commands",
  "dependencies": {
    "lz-string": "1.4.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "perfect-scrollbar": "0.7.1",
    "marked": "0.3.6"
  },
  "devDependencies": {
    "@types/google.analytics": "0.0.34",
    "@types/lz-string": "1.3.32",
    "@types/marked": "0.0.28",
    "@types/perfect-scrollbar": "0.7.0",
    "@types/react": "15.0.38",
    "@types/react-dom": "15.5.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^4.0.1",
    "rimraf": "2.6.1",
    "source-map-loader": "0.2.1",
    "ts-loader": "^2.1.0",
    "typescript": "^2.4.2",
    "uglify-js": "^2.8.29",
    "url-loader": "^0.5.7",
    "webpack": "^3.2.0",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.5.0"
  },
  "types": "./scripts/bin/",
  "scripts": {
    "clean": "rimraf scripts/bin modules/scripts/bin  modules/restheart/scripts/bin",
    "transpile-core": "node_modules/typescript/bin/tsc -p scripts",
    "transpile-modules": "tsc -p modules/scripts",
    "transpile": "run-s transpile-core transpile-modules",
    "transpile-core-watch": "npm run transpile-core -- --watch",
    "transpile-modules-watch": "npm run transpile-modules -- --watch",
    "transpile-watch": "run-p transpile-core-watch transpile-modules-watch",
    "serve": "webpack-dev-server --history-api-fallback --hot --inline --progress --colors --port 3330",
    "dev": "npm-run-all clean transpile -p transpile-watch serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --env.prod --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fugazi-io/webclient"
  },
  "author": "Nitzan Tomer <nitzan.tomer@gmail.com> (https://github.com/nitzantomer)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fugazi-io/webclient/issues"
  },
  "homepage": "http://fugazi.io"
}
